<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
             http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
             version="2.1">
  <persistence-unit name="plutoPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <class>com.vieira.pluto.entity.Uf</class>
    <class>com.vieira.pluto.entity.TipoEndereco</class>
    <class>com.vieira.pluto.entity.TipoPessoa</class>
    <class>com.vieira.pluto.entity.TipoTelefone</class>
    <class>com.vieira.pluto.entity.Pessoa</class>
    <class>com.vieira.pluto.entity.Telefone</class>
    <class>com.vieira.pluto.entity.Usuario</class>
    <class>com.vieira.pluto.entity.Sexo</class>
    <class>com.vieira.pluto.entity.Endereco</class>
    <class>com.vieira.pluto.entity.Municipio</class>
    <class>com.vieira.pluto.entity.EstadoCivil</class>
    <class>com.vieira.pluto.entity.Email</class>
    <class>com.vieira.pluto.entity.Cliente</class>
    <class>com.vieira.pluto.entity.ModeloVeiculo</class>
    <class>com.vieira.pluto.entity.Cor</class>
    <class>com.vieira.pluto.entity.PessoaVeiculo</class>
    <class>com.vieira.pluto.entity.Fabricante</class>
    <class>com.vieira.pluto.entity.TipoItemComercializavel</class>
    <class>com.vieira.pluto.entity.ItemComercializavel</class>
    <class>com.vieira.pluto.entity.FormaPagamento</class>
    <class>com.vieira.pluto.entity.StatusOrcamento</class>
    <class>com.vieira.pluto.entity.VeiculoOrcamento</class>
    <class>com.vieira.pluto.entity.Orcamento</class>
    <class>com.vieira.pluto.entity.ItemOrcamento</class>
    <class>com.vieira.pluto.entity.ItemVenda</class>
    <class>com.vieira.pluto.entity.Venda</class>
    <class>com.vieira.pluto.entity.VeiculoVenda</class>
    <class>com.vieira.pluto.entity.ParcelaContaReceber</class>
    <class>com.vieira.pluto.entity.ContaReceber</class>
    <class>com.vieira.pluto.entity.Parcela</class>
    <class>com.vieira.pluto.entity.ContaPagar</class>
    <class>com.vieira.pluto.entity.ParcelaContaPagar</class>
    <class>com.vieira.pluto.entity.HistoricoClassificacao</class>
    <class>com.vieira.pluto.entity.Classificacao</class>
    <class>com.vieira.pluto.entity.ConfiguracaoEmail</class>
    <class>com.vieira.pluto.entity.ConfiguracaoClassificacao</class>
    <class>com.vieira.pluto.entity.QuitacaoParcelaContaPagar</class>
    <class>com.vieira.pluto.entity.QuitacaoParcelaContaReceber</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/pluto"/>
      <property name="javax.persistence.jdbc.password" value="123"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.PostgreSQL95Dialect"/>
      <property name="hibernate.cache.provider_class" value="org.hibernate.cache.NoCacheProvider"/>
      <property name="hibernate.hbm2ddl.auto" value="none"/>
      <property name="hibernate.show_sql" value="false"/>
      <property name="hibernate.format_sql" value="true"/>
      <property name="hibernate.connection.autocommit" value="false"/>
    </properties>
  </persistence-unit>
</persistence>
